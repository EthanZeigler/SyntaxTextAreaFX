    {
        "include": {"context": [
            {"ref": "def:shebang"},
            {"ref": "def:shell-like-comment"},
            {"ref": "def:string"},
            {"ref": "def:single-quoted-string"},
            {"ref": "variable"},
            {"ref": "keyword"}
