            "then",
            "for",
            "foreach",
            "switch",
            "case",
            "while",
            "continue",
            "return",
            "break",
            "catch",
            "error",
            "eval",
            "uplevel",
            "after",
            "update",
            "vwait",
            "proc",
            "rename",
            "set",
