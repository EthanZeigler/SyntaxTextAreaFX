{"gtkrc.json": [
    {
        "include": {"context": [
            {"ref": "def:escape"},
            {"ref": "def:line-continue"}
        ]},
        "class-disabled": "no-spell-check",
        "start": "\"",
        "end": "\"",
        "id": "double-quoted-string",
        "class": "string",
        "style-ref": "string",
        "end-at-line-end": true
    },
    {
        "include": {"context": [
            {"ref": "def:escape"},
            {"ref": "def:line-continue"}
        ]},
        "class-disabled": "no-spell-check",
        "start": "'",
        "end": "'",
        "id": "single-quoted-string",
        "class": "string",
        "style-ref": "string",
        "end-at-line-end": true
    },
    {
        "include": {"context": [
            {"ref": "def:escape"},
            {"ref": "def:line-continue"}
        ]},
        "class-disabled": "no-spell-check",
        "start": "#",
        "id": "line-comment",
        "class": "comment",
        "style-ref": "comment",
        "end-at-line-end": true
    },
    {
        "match": "::|=",
        "id": "keysymbol",
        "style-ref": "keyword"
    },
    {
        "match": "\\b[0-9][0-9\\.]*\\b",
        "id": "number",
        "style-ref": "decimal"
    },
    {
        "id": "keyword",
        "keyword": [
            "style",
            "engine",
            "widget_class",
            "class",
            "widget"
        ],
        "style-ref": "keyword"
    },
    {
        "id": "variable",
        "keyword": [
            "fg",
            "bg",
            "bg_pixmap",
            "base",
            "text"
        ],
        "style-ref": "variable"
    },
    {
        "id": "state",
        "keyword": [
            "ACTIVE",
            "SELECTED",
            "NORMAL",
            "PRELIGHT",
            "INSENSITIVE"
        ],
        "style-ref": "state"
    },
    {
        "id": "include-directive",
        "keyword": "include",
        "style-ref": "include-directive"
    },
    {
        "id": "boolean-value",
        "keyword": [
            true,
            false
        ],
        "style-ref": "boolean-value"
    },
    {
        "include": {"context": [
            {"ref": "double-quoted-string"},
            {"ref": "single-quoted-string"},
            {"ref": "line-comment"},
            {"ref": "keysymbol"},
            {"ref": "number"},
            {"ref": "keyword"},
            {"ref": "variable"},
            {"ref": "state"},
            {"ref": "include-directive"},
            {"ref": "boolean-value"}
        ]},
        "id": "gtkrc",
        "class": "no-spell-check"
    }
]}