{"changelog.json": {
    "include": {"context": [
        {
            "include": {"context": [
                {
                    "sub-pattern": "date",
                    "style-ref": "date"
                },
                {
                    "sub-pattern": "name",
                    "style-ref": "name"
                },
                {
                    "sub-pattern": "email",
                    "style-ref": "email"
                }
            ]},
            "match": {
                "extended": true,
                "content": "^(?P<date>\\%{date})\\s+\n            (?P<name>.*)\\s*\n            (?P<email>\\<[^\\<]+\\>)\\s*$"
            },
            "id": "date-and-stuff",
            "class": "no-spell-check"
        },
        {
            "match": "^\\s*\\=.*\\=\\s*$",
            "id": "release",
            "class": "no-spell-check",
            "style-ref": "release"
        },
        {
            "include": {"context": {
                "where": "start",
                "sub-pattern": 1,
                "style-ref": "bullet"
            }},
            "start": "^\\s+(\\*)",
            "end": "\\:|^(?!\\S)",
            "id": "file",
            "class": "no-spell-check",
            "style-ref": "file"
        },
        {
            "match": "\\(\\s*\\%{identifier}\\s*\\)",
            "id": "function",
            "class": "no-spell-check",
            "style-ref": "function"
        }
    ]},
    "id": "changelog"
}}