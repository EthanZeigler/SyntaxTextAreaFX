{"netrexx.json": [
    {
        "include": {"context": {
            "match": "\\%{escaped-character}",
            "id": "escaped-character",
            "style-ref": "escaped-character"
        }},
        "class-disabled": "no-spell-check",
        "start": "[\"']",
        "end": "\\%{0@start}",
        "id": "string",
        "class": "string",
        "style-ref": "string",
        "end-at-line-end": true
    },
    {
        "match": "'(\\%{escaped-character}|.)'",
        "id": "char",
        "style-ref": "char"
    },
    {
        "include": {"context": {"ref": "def:in-comment"}},
        "class-disabled": "no-spell-check",
        "start": "--",
        "id": "line-comment",
        "class": "comment",
        "style-ref": "line-comment",
        "end-at-line-end": true
    },
    {
        "id": "externals",
        "keyword": [
            "package",
            "import",
            "options"
        ],
        "style-ref": "externals"
    },
    {
        "id": "options",
        "keyword": [
            "binary",
            "nobinary",
            "comments",
            "nocomments",
            "compact",
            "nocompact",
            "crossref",
            "nocrossref",
            "decimal",
            "nodecimal",
            "diag",
            "nodiag",
            "explicit",
            "noexplicit",
            "format",
            "noformat",
            "java",
            "nojava",
            "logo",
            "nologo",
            "replace",
            "noreplace",
            "sourcedir",
            "nosourcedir",
            "strictargs",
            "nostrictargs",
            "strictassign",
            "nostrictassign",
            "strictcase",
            "nostrictcase",
            "strictimport",
            "nostrictimport",
            "strictprops",
            "nostrictprops",
            "strictsignal",
            "nostrictsignal",
            "symbols",
            "nosymbols",
            "notrace",
            "trace1",
            "trace2",
            "verbose",
            "verbose0",
            "verbose1",
            "verbose2",
            "verbose3",
            "verbose4",
            "verbose5",
            "utf8",
            "noutf8"
        ],
        "style-ref": "options"
    },
    {
        "id": "primitive-types",
        "keyword": [
            "boolean",
            "byte",
            "char",
            "double",
            "float",
            "int",
            "long",
            "short"
        ],
        "style-ref": "primitive-types"
    },
    {
        "id": "special",
        "keyword": [
            "ask",
            "class",
            "digits",
            "form",
            "length",
            null,
            "source",
            "sourceline",
            "super",
            "this",
            "trace",
            "version"
        ],
        "style-ref": "special"
    },
    {
        "id": "visibility",
        "keyword": [
            "inheritable",
            "private",
            "public",
            "shared"
        ],
        "style-ref": "visibility"
    },
    {
        "id": "modifiers",
        "keyword": [
            "abstract",
            "adapter",
            "constant",
            "final",
            "interface",
            "native",
            "static",
            "transient",
            "volatile",
            "protect",
            "binary",
            "deprecated",
            "returns",
            "signals",
            "extends",
            "uses",
            "implements",
            "unused"
        ],
        "style-ref": "modifiers"
    },
    {
        "id": "keywords",
        "keyword": [
            "catch",
            "do",
            "else",
            "end",
            "exit",
            "finally",
            "if",
            "iterate",
            "leave",
            "loop",
            "method",
            "nop",
            "numeric",
            "otherwise",
            "parse",
            "properties",
            "return",
            "say",
            "select",
            "signal",
            "then",
            "when"
        ],
        "style-ref": "keywords"
    },
    {
        "id": "sub-keywords",
        "keyword": [
            "all",
            "by",
            "case",
            "engineering",
            "for",
            "forever",
            "label",
            "methods",
            "off",
            "over",
            "results",
            "scientific",
            "to",
            "until",
            "var",
            "while"
        ],
        "style-ref": "sub-keywords"
    },
    {
        "id": "builtin-function",
        "keyword": [
            "abbrev",
            "abs",
            "b2x",
            "center",
            "centre",
            "changestr",
            "compare",
            "copies",
            "copyindexed",
            "countstr",
            "c2d",
            "c2x",
            "datatype",
            "delstr",
            "delword",
            "d2c",
            "d2x",
            "exists",
            "format",
            "insert",
            "lastpos",
            "left",
            "lower",
            "max",
            "min",
            "overlay",
            "pos",
            "right",
            "sequence",
            "sign",
            "space",
            "strip",
            "substr",
            "subword",
            "translate",
            "trunc",
            "upper",
            "verify",
            "word",
            "wordindex",
            "wordlength",
            "wordpos",
            "words",
            "x2b",
            "x2c",
            "x2d"
        ],
        "style-ref": "builtin-function"
    },
    {
        "match": {
            "extended": true,
            "content": "(\\b([0-9]+|0[xX][0-9a-fA-F]+)[Ll]?\\b|\n         \\b(([0-9]+[Ee][-]?[0-9]+|\n            ([0-9]*\\.[0-9]+|[0-9]+\\.)([Ee][-]?[0-9]+)?)[fFdD]?|\n            [0-9]+[FfDd]))"
        },
        "id": "numeric",
        "style-ref": "numeric"
    },
    {
        "include": {"context": [
            {"ref": "string"},
            {"ref": "char"},
            {"ref": "line-comment"},
            {"ref": "def:c-like-comment-multiline"},
            {"ref": "externals"},
            {"ref": "options"},
            {"ref": "primitive-types"},
            {"ref": "special"},
            {"ref": "visibility"},
            {"ref": "modifiers"},
            {"ref": "keywords"},
            {"ref": "sub-keywords"},
            {"ref": "builtin-function"},
            {"ref": "numeric"}
        ]},
        "id": "netrexx",
        "class": "no-spell-check"
    }
]}