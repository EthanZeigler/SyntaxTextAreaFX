{"gtk-doc.json": [
    {
        "match": "^\\s*\\*(?!/)\\s*",
        "id": "skip-asterisk"
    },
    {
        "include": {"context": [
            {"ref": "skip-asterisk"},
            {"ref": "def:in-comment"},
            {
                "match": "\\@[\\w_]+",
                "style-ref": "parameter"
            },
            {
                "match": "%[\\w_]+",
                "style-ref": "constant"
            },
            {
                "match": "\\#[\\w_]+",
                "style-ref": "type"
            },
            {
                "match": "[\\w_]+\\(\\s*\\)",
                "style-ref": "function"
            },
            {
                "match": "(Returns|Return value)\\:",
                "style-ref": "return"
            },
            {
                "match": "Since\\:",
                "style-ref": "since"
            },
            {
                "match": "Deprecated\\:",
                "style-ref": "deprecated"
            }
        ]},
        "id": "inline-docs-body"
    },
    {
        "include": {"context": [
            {"ref": "skip-asterisk"},
            {
                "include": {"context": [
                    {
                        "where": "start",
                        "sub-pattern": 2,
                        "style-ref": "function-name"
                    },
                    {
                        "where": "start",
                        "sub-pattern": 3,
                        "style-ref": "signal-name"
                    },
                    {
                        "where": "start",
                        "sub-pattern": 4,
                        "style-ref": "property-name"
                    },
                    {"ref": "inline-docs-body"}
                ]},
                "start": "(([\\w_]+\\:\\:[\\w_-]+)|([\\w_]+\\:[\\w_-]+)|([\\w_]+))\\:\\s*$",
                "extend-parent": false
            },
            {"ref": "inline-docs-body"}
        ]},
        "class-disabled": "no-spell-check",
        "start": "/\\*\\*(?!\\S)",
        "end": "\\*/",
        "id": "inline-docs-section",
        "class": "comment",
        "style-ref": "inline-docs-section"
    }
]}