{"ini.json": [
    {
        "class-disabled": "no-spell-check",
        "start": ";|#",
        "id": "line-comment",
        "class": "comment",
        "style-ref": "comment",
        "end-at-line-end": true
    },
    {
        "start": "^\\[",
        "end": "\\]$",
        "id": "group",
        "style-ref": "keyword"
    },
    {
        "match": "^_?X\\-[a-zA-Z\\-]+",
        "id": "non-standard-key",
        "style-ref": "non-standard-key"
    },
    {
        "match": "\\[[a-zA-Z_]+\\]",
        "id": "language",
        "style-ref": "decimal"
    },
    {
        "match": "^[a-zA-Z_][a-zA-Z0-9_.-]*",
        "id": "variable",
        "style-ref": "variable"
    },
    {
        "id": "boolean-value",
        "keyword": [
            true,
            false,
            "on",
            "off",
            "yes",
            "no"
        ],
        "style-ref": "boolean-value"
    },
    {
        "start": "'",
        "end": "'",
        "id": "single-quoted-string",
        "style-ref": "string"
    },
    {
        "class-disabled": "no-spell-check",
        "start": "\"",
        "end": "\"",
        "id": "double-quoted-string",
        "class": "string",
        "style-ref": "string"
    },
    {
        "match": "\\b[0-9]+\\b",
        "id": "integer",
        "style-ref": "decimal"
    },
    {
        "match": "(\\b[0-9]+(\\.[0-9]+)?|\\.[0-9]+)([Ee][\\+-]?[0-9]+)?\\b",
        "id": "decimal-number",
        "style-ref": "floating-point"
    },
    {
        "include": {"context": [
            {"ref": "line-comment"},
            {"ref": "group"},
            {"ref": "non-standard-key"},
            {"ref": "language"},
            {"ref": "variable"},
            {"ref": "boolean-value"},
            {"ref": "single-quoted-string"},
            {"ref": "double-quoted-string"},
            {"ref": "decimal-number"},
            {"ref": "integer"}
        ]},
        "id": "ini",
        "class": "no-spell-check"
    }
]}