{"yaml.json": [
    {
        "include": {"context": [
            {
                "end-parent": true,
                "start": "^(?'indent'\\s+)",
                "end": "^(?!\\%{indent@start})",
                "style-ref": "scalar"
            },
            {
                "end-parent": true,
                "match": "(?=.)"
            }
        ]},
        "class-disabled": "no-spell-check",
        "start": "(?<=^|\\s)[|>]$",
        "id": "scalar"
    },
    {
        "match": "(?<=^|\\s)\\*\\%{ischar}+(?=$|\\s)",
        "id": "alias",
        "style-ref": "alias"
    },
    {
        "match": "(?<=^|\\s)!(?:\\%{ischar}*)?!?\\%{ischar}+(?=$|\\s)",
        "id": "tag",
        "style-ref": "tag"
    },
    {
        "match": "(?<=^|\\s)&\\%{ischar}+(?=$|\\s)",
        "id": "anchor",
        "style-ref": "anchor"
    },
    {
        "class-disabled": "no-spell-check",
        "start": "(?<=^|\\%{lschar})(?'q'[\"'])",
        "end": "[^\\\\]\\%{q@start}",
        "id": "string",
        "end-at-line-end": true,
        "style-ref": "string"
    },
    {
        "class-disabled": "no-spell-check",
        "start": "(?=\\%{ischar}|[:-\\?\\]\\}]\\S)",
        "end": "(?=:\\s|:$|\\s#)",
        "id": "unquoted-string",
        "end-at-line-end": true,
        "style-ref": "string"
    },
    {
        "class-disabled": "no-spell-check",
        "start": "(?=\\%{ischar}|-\\S)",
        "end": "(?=[:,?\\[\\{\\]\\}]|\\s#)",
        "id": "inline-unquoted",
        "end-at-line-end": true,
        "style-ref": "string"
    },
    {
        "match": "(?:null)(?=\\s*$|\\s+#)",
        "id": null,
        "style-ref": null
    },
    {
        "match": "(?:null)(?=\\s*[:,?\\[\\{\\]\\}]|\\s+#)",
        "id": "inline-null",
        "style-ref": null
    },
    {
        "match": "(?:true|false)(?=\\s*$|\\s+#)",
        "id": "bool",
        "style-ref": "bool"
    },
    {
        "match": "(?:true|false)(?=\\s*[:,?\\[\\{\\]\\}]|\\s+#)",
        "id": "inline-bool",
        "style-ref": "bool"
    },
    {
        "match": "-?(?:0|[1-9][0-9]*)(?=\\s*[:,?\\[\\{\\]\\}]|\\s+#)",
        "id": "inline-int",
        "style-ref": "int"
    },
    {
        "match": "-?(?:0|[1-9][0-9]*)(?=\\s*$|\\s+#)",
        "id": "int",
        "style-ref": "int"
    },
    {
        "match": "-?(?:0|[1-9][0-9]*)(?:[.][0-9]*)?(?:[eE][-+]?[0-9]+)?(?=\\s*[:,?\\[\\{\\]\\}]|\\s+#)",
        "id": "inline-float",
        "style-ref": "float"
    },
    {
        "match": "-?(?:0|[1-9][0-9]*)(?:[.][0-9]*)?(?:[eE][-+]?[0-9]+)?(?=\\s*$|\\s+#)",
        "id": "float",
        "style-ref": "float"
    },
    {
        "match": "(?<=^|\\s)#.*$",
        "id": "comment",
        "style-ref": "comment"
    },
    {
        "include": {"context": [
            {"ref": "scalar"},
            {"ref": "inline-sequence"},
            {"ref": "inline-map"},
            {"ref": "comment"},
            {"ref": "alias"},
            {"ref": "tag"},
            {"ref": "anchor"},
            {"ref": "string"},
            {"ref": "inline-null"},
            {"ref": "inline-bool"},
            {"ref": "inline-int"},
            {"ref": "inline-float"},
            {"ref": "inline-unquoted"},
            {
                "end-parent": true,
                "match": "\\]"
            }
        ]},
        "start": "(?<=^|\\s)\\[",
        "id": "inline-sequence"
    },
    {
        "include": {"context": [
            {"ref": "scalar"},
            {"ref": "inline-sequence"},
            {"ref": "inline-map"},
            {"ref": "comment"},
            {"ref": "alias"},
            {"ref": "tag"},
            {"ref": "anchor"},
            {"ref": "string"},
            {"ref": "inline-null"},
            {"ref": "inline-bool"},
            {"ref": "inline-int"},
            {"ref": "inline-float"},
            {"ref": "inline-unquoted"},
