        "id": "map"
    },
    {
        "match": "^\\s*-\\s+",
        "id": "sequence"
    },
    {
        "match": "^(?:-{3}|[.]{3})(?:\\s+|$)",
        "id": "document",
        "style-ref": "comment"
    },
    {
        "match": "^%\\%{ischar}(?:\\%{ischar}|\\s+)*",
        "id": "directive",
        "style-ref": "directive"
    },
    {
        "include": {"context": [
            {"ref": "directive"},
            {"ref": "document"},
            {"ref": "scalar"},
            {"ref": "sequence"},
            {"ref": "inline-sequence"},
            {"ref": "inline-map"},
            {"ref": "comment"},
            {"ref": null},
            {"ref": "bool"},
            {"ref": "int"},
            {"ref": "float"},
            {"ref": "alias"},
            {"ref": "tag"},
            {"ref": "anchor"},
            {"ref": "map"},
            {"ref": "string"},
            {"ref": "unquoted-string"}
        ]},
        "id": "yaml",
        "class": "no-spell-check"
