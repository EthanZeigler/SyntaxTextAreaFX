        "style-ref": "type"
    },
    {
        "match": "i[0-9]+",
        "id": "integertype",
        "style-ref": "type"
    },
    {
        "match": "[%!][a-zA-Z$\\._0-9]*",
        "id": "variable",
        "style-ref": "variable"
    },
    {
        "match": "@[a-zA-Z$\\._][a-zA-Z$\\._0-9]*",
        "id": "identifier",
        "style-ref": "identifier"
    },
    {
        "match": {
            "extended": true,
            "content": "(\\b([0-9]+|0x[0-9a-fA-F]+)\\b|\n        \\b([0-9]*\\.[0-9]+[Ee][+-]?[0-9]+|\n        [0-9]+))"
        },
        "id": "numeric",
        "style-ref": "number"
    },
    {
        "start": "\"",
        "end": "\"",
        "id": "string",
        "style-ref": "string",
        "end-at-line-end": true
    },
    {
        "start": ";",
        "id": "line-comment",
        "style-ref": "comment",
        "end-at-line-end": true
    },
    {
        "include": {"context": [
            {"ref": "keywords"},
            {"ref": "functions"},
            {"ref": "constant"},
            {"ref": "boolean"},
            {"ref": "types"},
            {"ref": "integertype"},
            {"ref": "variable"},
            {"ref": "identifier"},
            {"ref": "numeric"},
            {"ref": "string"},
            {"ref": "line-comment"}
        ]},
        "id": "llvm"
    }
