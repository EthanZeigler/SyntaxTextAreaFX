{"json.json": [
    {
        "include": {"context": {
            "include": {"context": {"ref": "value"}},
            "start": "",
            "end": ",|\\s*(?=\\#93)"
        }},
        "start": "\\[",
        "end": "\\]",
        "id": "array"
    },
    {
        "include": {"context": [
            {
                "ref": "string",
                "style-ref": "keyname"
            },
            {
                "include": {"context": [
                    {
                        "where": "start",
                        "sub-pattern": 0,
                        "style-ref": "keyname"
                    },
                    {"ref": "value"}
                ]},
                "start": ":",
                "end": ",|\\s*(?=})"
            },
            {"ref": "catchall"}
        ]},
        "start": "\\{",
        "end": "\\}",
        "id": "object"
    },
    {
        "include": {"context": [
            {
                "match": "\\\\(?:[ntrfb\"\\\\/]|u[0-9a-fA-F]{4})",
                "id": "valid-escape",
                "style-ref": "special-char"
            },
            {
                "match": "\\\\(?:x[0-9a-fA-F]{2}|[0-3]?[0-7]{1,2}|.)",
                "id": "invalid-escape",
                "style-ref": "error"
            },
            {
                "start": "\\\\$",
                "end": "^",
                "id": "line-continue",
                "style-ref": "error"
            }
        ]},
        "class-disabled": "no-spell-check",
        "start": "\"",
        "end": "\"",
        "id": "string",
        "class": "string",
        "style-ref": "string",
        "end-at-line-end": true
    },
    {
        "match": "-?(?:[1-9][0-9]*|0)(?![.eE])",
        "id": "decimal",
        "style-ref": "decimal"
    },
    {
        "match": "-?(?:[1-9][0-9]*|0)(?:\\.[0-9]+)?(?:[eE][+\\-]?[0-9]+)?",
        "id": "float",
        "style-ref": "float"
    },
    {
        "match": "\\S",
        "id": "catchall",
        "style-ref": "error",
        "extend-parent": false
    },
    {
        "include": {"context": [
            {"ref": "object"},
            {"ref": "array"},
            {"ref": "string"},
            {"ref": "decimal"},
            {"ref": "float"},
            {
                "ref": "js:null-value",
                "style-ref": "null-value"
            },
            {
                "ref": "js:boolean",
                "style-ref": "boolean"
            },
            {"ref": "catchall"}
        ]},
        "id": "value"
    },
    {
        "include": {"context": {"ref": "value"}},
        "id": "json",
        "class": "no-spell-check"
    }
]}