    },
    {
        "include": {"context": [
            {"ref": "line-comment"},
            {"ref": "block-comment"},
            {"ref": "close-comment-outside-comment"},
            {"ref": "keywords"},
            {"ref": "attributes"}
        ]},
        "id": "dot",
        "class": "no-spell-check"
    }
