{"matlab.json": [
    {
        "include": {"context": [
            {"ref": "block-comment"},
            {"ref": "def:in-comment"}
        ]},
        "class-disabled": "no-spell-check",
        "start": "^\\s*%{\\s*$",
        "end": "^\\s*%}\\s*$",
        "id": "block-comment",
        "class": "comment",
        "style-ref": "comment"
    },
    {
        "include": {"context": {"ref": "def:in-comment"}},
        "class-disabled": "no-spell-check",
        "start": "%",
        "id": "line-comment",
        "class": "comment",
        "style-ref": "comment",
        "end-at-line-end": true
    },
    {
        "id": "builtin",
        "keyword": [
            "abs",
            "acos",
            "asin",
            "atan2",
            "atan",
            "ceil",
            "conv",
            "cosh",
            "cos",
            "cumprod",
            "dims",
            "error",
            "fclose",
            "floor",
            "fopen",
            "fprintf",
            "fread",
            "fsolve",
            "imag",
            "isempty",
            "isinf",
            "islogical",
            "ismatrix",
            "isnan",
            "isna",
            "isnumeric",
            "isscalar",
            "isstr",
            "isvector",
            "length",
            "linspace",
            "log10",
            "log2",
            "log",
            "max",
            "min",
            "printf",
            "prod",
            "real",
            "rem",
            "repmat",
            "reshape",
            "round",
            "setstr",
            "sinh",
