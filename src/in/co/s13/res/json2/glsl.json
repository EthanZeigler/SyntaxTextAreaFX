            "default",
            "attribute",
            "const",
            "in",
            "inout",
            "out",
            "uniform",
            "varying",
            "invariant",
            "centroid",
            "flat",
            "smooth",
            "noperspective",
            "layout",
            "patch",
            "sample",
            "subroutine",
            "lowp",
            "mediump",
            "highp",
            "precision"
        ],
        "style-ref": "keyword"
    },
    {
