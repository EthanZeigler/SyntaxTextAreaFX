{"glsl.json": [
    {
        "prefix": "^[ \\t]*#[ \\t]*",
        "id": "preprocessor",
        "keyword": [
            "define",
            "undef",
            "(if|elif)([ \\t]+defined)?",
            "ifdef",
            "ifndef",
            "else",
            "endif",
            "error",
            "pragma",
            "extension",
            "version",
            "line"
        ],
        "style-ref": "preprocessor"
    },
    {
        "id": "common-macros",
        "keyword": [
            "__VERSION__",
            "__LINE__",
            "__FILE__"
        ],
        "style-ref": "preprocessor"
    },
    {
        "id": "keywords",
        "keyword": [
            "break",
            "continue",
            "discard",
            "do",
            "for",
            "if",
            "else",
            "return",
            "while",
            "switch",
