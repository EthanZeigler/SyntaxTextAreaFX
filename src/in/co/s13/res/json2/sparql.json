            "xsd:int",
            "xsd:integer",
            "xsd:language",
            "xsd:long",
            "xsd:negativeInteger",
            "xsd:nonNegativeInteger",
            "xsd:nonPositiveInteger",
            "xsd:normalizedString",
            "xsd:precisionDecimal",
            "xsd:positiveInteger",
            "xsd:short",
            "xsd:string",
            "xsd:time",
            "xsd:token",
            "xsd:unsignedByte",
            "xsd:unsignedInt",
            "xsd:unsignedLong",
            "xsd:unsignedShort",
            "xsd:yearMonthDuration",
            "dcterms:Box",
