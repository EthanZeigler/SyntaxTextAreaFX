            {"ref": "namespace"},
            {
                "ref": "xml:namespace",
                "original": true
            }
        ]},
        "id": "xslt-namespace"
    },
    {
        "once-only": true,
        "id": "elements",
        "suffix": "\\b(?!\\s*=)",
