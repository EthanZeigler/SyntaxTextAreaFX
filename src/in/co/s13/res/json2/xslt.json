            "character-map",
            "choose",
            "comment",
            "copy-of",
            "copy",
            "decimal-format",
            "document",
            "element",
            "fallback",
            "for-each-group",
            "for-each",
            "function",
            "if",
            "import-schema",
            "import",
            "include",
            "key",
            "matching-substring",
            "message",
            "namespace-alias",
            "namespace",
            "number",
            "next-match",
            "non-matching-substring",
            "otherwise",
            "output-character",
            "output",
            "param",
            "perform-sort",
            "preserve-space",
            "processing-instruction",
            "result-document",
            "script",
            "sequence",
            "sort",
            "strip-space",
            "stylesheet",
            "template",
            "text",
            "transform",
            "value-of",
            "variable",
