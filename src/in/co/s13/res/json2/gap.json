{"gap.json": [
    {
        "match": "\\'(\\\\[trnb\\\\\\'\\\"]|[^\\\\])\\'",
        "id": "character",
        "style-ref": "character"
    },
    {
        "id": "keyword",
        "keyword": [
            "and",
            "break",
            "continue",
            "do",
            "elif",
            "else",
            "end",
            "fail",
            false,
            "fi",
            "for",
            "function",
            "if",
            "in",
            "infinity",
            "local",
            "not",
            "od",
            "or",
            "rec",
            "repeat",
            "return",
            "then",
            true,
            "until",
            "while"
        ],
        "style-ref": "keyword"
    },
    {
        "match": "\\b[0-9]+\\b",
        "id": "decimal",
        "style-ref": "decimal"
    },
    {
        "include": {"context": [
            {"ref": "def:shell-like-comment"},
            {"ref": "def:string"},
            {"ref": "character"},
            {"ref": "keyword"},
            {"ref": "decimal"}
        ]},
        "id": "gap"
    }
]}