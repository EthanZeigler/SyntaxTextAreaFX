    "_name": "COBOL",
    "_section": "Source",
    "styles": {"style": [
        {
            "_name": "Comment",
            "map-to": "def:comment",
            "id": "comment"
        },
        {
            "_name": "Statement",
            "map-to": "def:statement",
            "id": "statement"
        },
        {
            "_name": "Variable",
            "map-to": "def:type",
            "id": "variable"
