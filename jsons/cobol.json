        {
            "_name": "Keyword",
            "map-to": "def:keyword",
            "id": "keyword"
        },
        {
            "_name": "Constant",
            "map-to": "def:constant",
            "id": "constant"
        },
        {
            "_name": "String",
            "map-to": "def:string",
            "id": "string"
        },
        {
            "_name": "Number",
            "map-to": "def:decimal",
            "id": "number"
        },
        {
            "_name": "Operator",
            "map-to": "def:operator",
            "id": "operator"
        },
        {
            "_name": "Error",
            "map-to": "def:error",
            "id": "error"
        }
    ]},
    "id": "cobol",
    "version": 2,
    "definitions": {"context": [
        {
            "include": {"context": {"ref": "def:in-comment"}},
            "class-disabled": "no-spell-check",
            "start": "^\\s{0,6}\\*",
            "id": "line-comment",
            "class": "comment",
            "style-ref": "comment",
            "end-at-line-end": true
        },
        {
            "class-disabled": "no-spell-check",
            "start": "^\\s{0,6}\\w",
            "id": "bad-ident",
            "class": "error",
            "style-ref": "error",
            "end-at-line-end": true
        },
        {
            "match": ".{74,}",
            "id": "bad-long-line",
            "style-ref": "error"
        },
        {
            "class-disabled": "no-spell-check",
            "start": "(\\*)(?=(?:[^\"']|[\"|'][^\"']*\")*$)",
            "id": "bad-comment-line",
            "class": "error",
            "style-ref": "error",
            "end-at-line-end": true
        },
        {
            "match": "\\b([A-Za-z0-9_]*)\\b",
            "id": "variable",
            "style-ref": "variable"
        },
        {
            "class-disabled": "no-spell-check",
            "start": "\"",
            "end": "\"",
            "id": "string-double-quote",
            "class": "string",
            "style-ref": "string",
            "end-at-line-end": true
        },
        {
            "class-disabled": "no-spell-check",
            "start": "'",
            "end": "'",
            "id": "string-single-quote",
            "class": "string",
            "style-ref": "string",
            "end-at-line-end": true
        },
        {
            "match": {
                "extended": true,
                "content": "\\b[0-9]+(\\.[0-9]+)?\\b"
            },
            "id": "number",
            "style-ref": "number"
        },
