{"language": {
    "metadata": {"property": [
        {
            "name": "mimetypes",
            "content": "text/x-bibtex"
        },
        {
            "name": "globs",
            "content": "*.bib"
        },
        {
            "name": "line-comment-start",
            "content": "%"
        }
    ]},
    "_name": "BibTeX",
    "_section": "Markup",
    "styles": {"style": [
        {
            "_name": "Entries",
            "map-to": "def:keyword",
            "id": "entry-type"
        },
        {
            "_name": "Field",
            "map-to": "def:identifier",
            "id": "field"
        }
    ]},
    "id": "bibtex",
    "version": 2,
    "definitions": {"context": [
        {
            "prefix": "@",
            "id": "entry-type",
            "keyword": [
                "book",
                "article",
                "booklet",
                "conference",
                "inbook",
                "incollection",
                "inproceedings",
                "manual",
                "mastersthesis",
                "lambda",
                "misc",
                "phdthesis",
                "proceedings",
                "techreport",
                "unpublished"
            ],
            "class": "no-spell-check",
            "style-ref": "entry-type"
        },
        {
            "id": "field",
            "keyword": [
                "author",
                "title",
                "journal",
                "year",
                "volume",
                "number",
                "pages",
                "month",
                "note",
                "key",
                "publisher",
                "editor",
                "series",
                "address",
                "edition",
                "howpublished",
                "booktitle",
                "organization",
                "chapter",
                "school",
                "institution",
                "type"
            ],
            "class": "no-spell-check",
            "style-ref": "field"
        },
        {
            "include": {"context": [
                {"ref": "latex:latex"},
                {"ref": "entry-type"},
                {"ref": "field"}
            ]},
            "id": "bibtex"
        }
    ]},
    "default-regex-options": {"case-sensitive": false}
}}